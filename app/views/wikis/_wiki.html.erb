<tr>
  <td id="wiki-title">
    <% if policy(wiki).show? %>
      <%= link_to wiki.title, wiki_path(wiki.id, wiki)%>
    <% else %>
      <%= wiki.title %>
    <% end %>
  </td>
  <td id="wiki-edit"> 
    <% if policy(wiki).edit? %>
      <%= link_to "Edit", edit_wiki_path(wiki.id, wiki)%>
    <% end %>
  </td>
  <td id="wiki-delete">
    <% if policy(wiki).destroy? %>
      <%= link_to "Delete", [wiki], method: :delete,data: {confirm: 'Are you sure you want to delete this post?'}%>
    <% end %>
</tr>