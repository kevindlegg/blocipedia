<tr>
  <td id="wiki-title"><%= link_to wiki.title, wiki_path(wiki.id, wiki)%></td>
  <% if current_user && (wiki.user == current_user || current_user.admin?) %>
    <td id="wiki-edit"> <%= link_to "Edit", edit_wiki_path(wiki.id, wiki)%> </td>
    <td id="wiki-delete"> <%= link_to "Delete", [wiki], method: :delete,data: {confirm: 'Are you sure you want to delete this post?'}%> </td>
  <% else %>
    <td id="wiki-edit"> </td>
    <td id="wiki-delete"> </td>
  <% end %>
</tr>